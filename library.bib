@article{dehbozorgiQuantitativeAnalysisRelative2010,
  title = {Quantitative Analysis of Relative Tectonic Activity in the {{Sarvestan}} Area, Central {{Zagros}}, {{Iran}}},
  author = {Dehbozorgi, M. and Pourkermani, M. and Arian, M. and Matkan, A.A. and Motamedi, H. and Hosseiniasl, A.},
