@article{stotlerOriginStabilityPermafrost2010,
  title = {Origin and Stability of a Permafrost Methane Hydrate Occurrence in the {{Canadian Shield}}},
  author = {Stotler, Randy L. and Frape, Shaun K. and Ahonen, Lasse and Clark, Ian and Greene, Shane and Hobbs, Monique and Johnson, Elizabeth and Lemieux, Jean-Michel and Peltier, Richard and Pratt, Lisa and Ruskeeniemi, Timo and Sudicky, Ed and Tarasov, Lev},
  date = {2010-08},
