@article{agliardiStructuralConstraintsDeepseated2001,
  title = {Structural Constraints on Deep-Seated Slope Deformation Kinematics},
  author = {Agliardi, F. and Crosta, G. and Zanchi, A.},
  date = {2001-01},
  journaltitle = {Engineering Geology},
  shortjournal = {Engineering Geology},
