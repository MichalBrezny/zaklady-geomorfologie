@article{kacprzakRoleFlyschSandstones2015,
  title = {The Role of Flysch Sandstones in Forming the Properties of Cover Deposits and Soils - Examples from the {{Carpathians}}},
  author = {Kacprzak, A. and Szymański, W. and Wójcik-Tabol, P.},
