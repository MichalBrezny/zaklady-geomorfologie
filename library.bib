  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Radiocarbon\\2009\\Bronk Ramsey_2009_Bayesian Analysis of Radiocarbon Dates.pdf}
}

