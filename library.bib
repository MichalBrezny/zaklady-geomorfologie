@article{turcotteGeneralLandslideDistribution2006,
  title = {A General Landslide Distribution Applied to a Small Inventory in {{Todi}}, {{Italy}}},
  author = {Turcotte, Donald L. and Malamud, Bruce D. and Guzzetti, Fausto and Reichenbach, Paola},
  date = {2006},
  journaltitle = {Geological Society, London, Special Publications},
