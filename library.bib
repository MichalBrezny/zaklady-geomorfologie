@article{szaboIntensiveWoodlandManagement2015,
  title = {Intensive Woodland Management in the {{Middle Ages}}: Spatial Modelling Based on Archival Data},
  shorttitle = {Intensive Woodland Management in the {{Middle Ages}}},
  author = {Szabó, Péter and Müllerová, Jana and Suchánková, Silvie and Kotačka, Martin},
  date = {2015-04},
