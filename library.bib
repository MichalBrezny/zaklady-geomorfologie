@article{wagnerCorrelationsCaveLevels2011,
  title = {Correlations of Cave Levels, Stream Terraces and Planation Surfaces along the {{River Mur}}—{{Timing}} of Landscape Evolution along the Eastern Margin of the {{Alps}}},
  author = {Wagner, Thomas and Fritz, Harald and Stüwe, Kurt and Nestroy, Othmar and Rodnight, Helena and Hellstrom, John and Benischke, Ralf},
  date = {2011-11},
  journaltitle = {Geomorphology},
  shortjournal = {Geomorphology},
