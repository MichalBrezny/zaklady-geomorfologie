@article{panekCosmogenicAgeConstraints2016,
  title = {Cosmogenic Age Constraints on Post-{{LGM}} Catastrophic Rock Slope Failures in the {{Tatra Mountains}} ({{Western Carpathians}})},
  author = {Pánek, Tomáš and Engel, Zbyněk and Mentlík, Pavel and Braucher, Régis and Břežný, Michal and Škarpich, Václav and Zondervan, Albert},
  date = {2016-03},
  journaltitle = {CATENA},
  shortjournal = {CATENA},
