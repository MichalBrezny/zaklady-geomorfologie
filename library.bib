@article{heinChronologyLastGlacial2010,
  title = {The Chronology of the {{Last Glacial Maximum}} and Deglacial Events in Central {{Argentine Patagonia}}},
  author = {Hein, Andrew S. and Hulton, Nicholas R. J. and Dunai, Tibor J. and Sugden, David E. and Kaplan, Michael R. and Xu, Sheng},
  date = {2010-05-01},
  journaltitle = {Quaternary Science Reviews},
  shortjournal = {Quaternary Science Reviews},
  volume = {29},
  number = {9},
  pages = {1212--1227},
  issn = {0277-3791},
  doi = {10/b85shd},
  url = {https://www.sciencedirect.com/science/article/pii/S0277379110000326},
  urldate = {2022-01-05},
  abstract = {This paper evaluates the chronology of the last glacial cycle and deglaciation in the Lago Pueyrredón valley of central Patagonia, 47.5° S, Argentina. The valley was a major outlet of the former Patagonian Ice Sheet and the moraines that record its fluctuations are an important proxy record of climate change in southern South America. Such moraines are well-preserved in the Lago Pueyrredón valley owing in part to the semi-arid environment east of the mountain front. Here, we provide the first direct chronology for the age of the “Rio Blanco” moraine system by utilizing cosmogenic-nuclide surface exposure ages. Boulders on the moraines give 10Be exposure ages that indicate the Last Glacial Maximum (LGM) maximum extent occurred by 27–25ka. Subsequent advances occurred at 23–22ka, 20–18ka, and ca. 18–17ka. Initial deglaciation began after ca. 18–17ka and was interrupted as evidenced by the Lago Columna moraines up-valley. Subsequently the outlet glaciers occupying both the Lago Pueyrredón basin (Chilean name: Lago Cochrane) and the Lago Buenos Aires basin (Chilean name: Lago General Carrera) to the north, rapidly retreated more than 80km at around 16.5–15ka. The timing of the LGM maximum extent and the onset of deglaciation occurred broadly synchronously throughout Patagonia. Deglaciation resulted in a series of interconnected glacier-dammed lakes in the region that initially drained toward the Atlantic Ocean and later drained to the Pacific Ocean as a consequence of disintegrating ice in the Andes.},
  langid = {english},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Quaternary Science Reviews\\2010\\Hein et al_2010_The chronology of the Last Glacial Maximum and deglacial events in central.pdf}
}

