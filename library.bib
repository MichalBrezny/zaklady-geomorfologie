  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Geomorphology\\2010\\Chiessi et al_2010_Geological, geomechanical and geostatistical assessment of rockfall hazard in.pdf}
