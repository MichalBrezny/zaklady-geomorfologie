@article{baronStructureDynamicsDeepseated2004,
  title = {Structure and Dynamics of Deep-Seated Slope Failures in the {{Magura Flysch Nappe}}, Outer {{Western Carpathians}} ({{Czech Republic}})},
  author = {Baroň, Ivo and Cílek, V. and Krejčí, Oldřich and Melichar, R. and Hubatka, F.},
