@article{colesantiInvestigatingLandslidesSpaceborne2006,
  title = {Investigating Landslides with Space-Borne {{Synthetic Aperture Radar}} ({{SAR}}) Interferometry},
  author = {Colesanti, Carlo and Wasowski, Janusz},
  date = {2006-12},
  journaltitle = {Engineering Geology},
