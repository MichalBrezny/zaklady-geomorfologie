@article{roessnerPotentialSatelliteRemote2005,
  title = {Potential of {{Satellite Remote Sensing}} and {{GIS}} for {{Landslide Hazard Assessment}} in {{Southern Kyrgyzstan}} ({{Central Asia}})},
  author = {Roessner, Sigrid and Wetzel, Hans-Ulrich and Kaufmann, Hermann and Sarnagoev, Aman},
  date = {2005-07},
  journaltitle = {Natural Hazards},
  shortjournal = {Nat Hazards},
  volume = {35},
