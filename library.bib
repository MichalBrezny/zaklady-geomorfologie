@article{mereuStudyRelationsML2020,
  title = {A Study of the Relations between {{ML}}, {{Me}}, {{Mw}}, Apparent Stress, and Fault Aspect Ratio},
  author = {Mereu, Robert F.},
  date = {2020-01-01},
  journaltitle = {Physics of the Earth and Planetary Interiors},
  shortjournal = {Physics of the Earth and Planetary Interiors},
  volume = {298},
