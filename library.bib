  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Natural Hazards and Earth System Sciences\\2007\\Ardizzone et al_2007_Identification and mapping of recent rainfall-induced landslides using.pdf}
