@article{sajinkumarWeatheringLandslideOccurrences2011,
  title = {Weathering and Landslide Occurrences in Parts of {{Western Ghats}}, {{Kerala}}},
  author = {Sajinkumar, K. S. and Anbazhagan, S. and Pradeepkumar, A. P. and Rani, V. R.},
  date = {2011-09},
  journaltitle = {Journal of the Geological Society of India},
  shortjournal = {J Geol Soc India},
  volume = {78},
