@article{pragerAgeDistributionFossil2008,
  title = {Age Distribution of Fossil Landslides in the {{Tyrol}} ({{Austria}}) and Its Surrounding Areas},
  author = {Prager, C. and Zangerl, C. and Patzelt, G. and Brandner, R.},
  date = {2008-04-24},
