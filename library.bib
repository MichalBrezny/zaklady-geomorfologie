@article{strouthIntegratedBackForward2009,
  title = {Integrated Back and Forward Analysis of Rock Slope Stability and Rockslide Runout at {{Afternoon Creek}}, {{Washington}}},
  author = {Strouth, Alex and Eberhardt, Erik},
