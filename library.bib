@article{valletFunctioningPrecipitationdisplacementModelling2016,
  title = {Functioning and Precipitation-Displacement Modelling of Rainfall-Induced Deep-Seated Landslides Subject to Creep Deformation},
  author = {Vallet, A. and Charlier, J. B. and Fabbri, O. and Bertrand, C. and Carry, N. and Mudry, J.},
