}

@book{aversaLandslidesEngineeredSlopes2016,
  title = {Landslides and {{Engineered Slopes}}. {{Experience}}, {{Theory}} and {{Practice}}: {{Proceedings}} of the 12th {{International Symposium}} on {{Landslides}} ({{Napoli}}, {{Italy}}, 12-19 {{June}} 2016)},
