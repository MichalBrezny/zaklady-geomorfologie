@article{andersenWidespreadErosionHigh2018,
  title = {Widespread Erosion on High Plateaus during Recent Glaciations in {{Scandinavia}}},
  author = {Andersen, Jane L. and Egholm, David L. and Knudsen, Mads F. and Linge, Henriette and Jansen, John D. and Pedersen, Vivi K. and Nielsen, Søren B. and Tikhomirov, Dmitry and Olsen, Jesper and Fabel, Derek and Xu, Sheng},
  date = {2018-02-26},
  journaltitle = {Nature Communications},
  volume = {9},
