@article{martinottiControllingFactorsDeepseated2011,
  title = {Controlling Factors for Deep-Seated Gravitational Slope Deformation ({{DSGSD}}) in the {{Aosta Valley}} ({{NW Alps}}, {{Italy}})},
  author = {Martinotti, G. and Giordan, D. and Giardino, M. and Ratto, S.},
  date = {2011},
  journaltitle = {Geological Society, London, Special Publications},
  shortjournal = {Geological Society, London, Special Publications},
