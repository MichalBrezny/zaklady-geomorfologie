@article{kanamoriMagnitudeScaleQuantification1983,
  title = {Magnitude Scale and Quantification of Earthquakes},
  author = {Kanamori, Hiroo},
  date = {1983-04-10},
  journaltitle = {Tectonophysics},
  shortjournal = {Tectonophysics},
  series = {Quantification of {{Earthquakes}}},
