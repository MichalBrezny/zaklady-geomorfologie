@article{palumboTopographicLithologicControl2010,
  title = {Topographic and Lithologic Control on Catchment-Wide Denudation Rates Derived from Cosmogenic {{10Be}} in Two Mountain Ranges at the Margin of {{NE Tibet}}},
  author = {Palumbo, L. and Hetzel, R. and Tao, M. and Li, X.},
