@article{panekHLAVATARIDGEMORAVIANSILESIAN,
  title = {{{HLAVATÁ RIDGE}}, {{MORAVIAN-SILESIAN BESKIDS}} ({{CZECH REPUBLIC}})},
  author = {Pánek, Tomáš and Smolková, Veronika and Hradecký, Jan and Šilhán, Karel},
  journaltitle = {MORAVIAN GEOGRAPHICAL REPORTS},
  volume = {17},
  pages = {12},
  abstract = {Frontal parts of nappes and overthrusts belong to the most exposed environments in the evolution of slope deformations. The frontal part of the Magura nappe creates a distinct morphotectonic and lithological boundary characterized by the occurrence of various types of slope deformations. A representative example of such a geomorphological situation can be found on the northern slopes of the Hlavatá Ridge in the Moravian-Silesian Beskids. Radiocarbon dating of peat bogs and lacustrine deposits of a fossil landslidedammed lake enabled us to determine the chronology of a retrogressive landslide. The main phase of the landslide evolution took place \textasciitilde 1.6 ka BP; the secondary distinctly retrogressive reactivation of the landslide occurred in 0.8–0.9 ka BP. Our case study demonstrates dynamic Late Holocene geomorphic evolution in the area of the Flysch Carpathians and re-evaluates the chronology of landforms previously considered as of the Pleistocene age.},
  langid = {english},
  keywords = {⛔ No DOI found},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\MORAVIAN GEOGRAPHICAL REPORTS\\undefined\\Pánek et al_HLAVATÁ RIDGE, MORAVIAN-SILESIAN BESKIDS (CZECH REPUBLIC).pdf}
}

@article{panekHoloceneReactivationsCatastrophic2013,
  title = {Holocene Reactivations of Catastrophic Complex Flow-like Landslides in the {{Flysch Carpathians}} ({{Czech Republic}}/{{Slovakia}})},
