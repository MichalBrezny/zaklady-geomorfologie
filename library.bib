@article{dehlsNeotectonicFaultingNorthern2000,
  title = {Neotectonic Faulting in Northern {{Norway}}; the {{Stuoragurra}} and {{Nordmannvikdalen}} Postglacial Faults},
  author = {Dehls, John F. and Olesen, Odleiv and Olsen, Lars and Harald Blikra, Lars},
  date = {2000-10},
  journaltitle = {Quaternary Science Reviews},
  shortjournal = {Quaternary Science Reviews},
  volume = {19},
  number = {14-15},
  pages = {1447--1460},
  issn = {02773791},
  doi = {10/fdhf9n},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0277379100000731},
  urldate = {2020-01-10},
  abstract = {A systematic compilation and characterisation of many reports of neotectonic crustal deformation in Norway (both on local and regional scales) has identi"ed two neotectonic faults in northern Norway. The Stuoragurra Fault is a large reverse fault in Finnmark County. The Nordmannvikdalen fault is a much smaller normal fault in Troms County. The Stuoragurra postglacial fault can be followed, in several discontinuous sections, for 80 km, in a NE\vphantom\{\}SW direction. The fault has up to 10 m of displacement. During 1998, two trenches were made across the fault. The hanging wall was seen to be thrust upwards over the footwall, with 7 m vertical displacement evident from displaced glacial contacts. The fault did not penetrate the overlying glacial materials, but rather folded them, forming a blind thrust. Large liquefaction and other deformation structures were found in the glacio\#uvial sediments in both trenches. Veins of angular and subangular pebbles from the local bedrock penetrate more than 10 m laterally from the thrust plane and into the sediments in the footwall. It is thought that these veins were injected during the fault activity. The major deformation of the sediments has a deH collement plane that continues laterally in the E/B horizon contact of the modern soil on top of the footwall. This may indicate that an initial pedogenesis had taken place before the fault activity occurred, however no macro plant fossils to support this were found in the possible buried soil. Deformational structures seen in the trench can be explained as a result of one major fault event. The Nordmannvikdalen postglacial fault is a NW\vphantom\{\}SE trending normal fault, dipping to the NE. The fault o!sets till on the NW slope of Nordmannvikdalen. The escarpment varies in height from 0.5 to 1.5 m, with a trench often present between the hanging wall and the footwall. The fault locally splits into two subparallel branches, however this is probably only in the glacial overburden. Ground penetrating radar (GPR) pro"les show two prominent bedrock re\#ectors, dipping approximately 403 to the NE. An upward extrapolation of the northernmost re\#ector through the overburden coincides with the position of the surface scarp. The bedrock is well exposed along a streamed parallel to the GPR pro"les. Bedding and foliation cannot be responsible for the re\#ectors visible in the GPR pro"les. Gravitational forces have been ruled out as the origin of the fault. Although both faults probably formed shortly after Weichselian deglaciation, there is evidence that isostatic uplift was not the only force acting. Quaternary tilting of Fennoscandia due to ridge push, or mantle convection, may also play a role in neotectonic activity in Norway. 2000 Elsevier Science Ltd. All rights reserved.},
  langid = {english},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Quaternary Science Reviews\\2000\\Dehls et al_2000_Neotectonic faulting in northern Norway\; the Stuoragurra and Nordmannvikdalen.pdf}
}

@article{dehnModellingFutureLandslide1999,
  title = {Modelling Future Landslide Activity Based on General Circulation Models},
  author = {Dehn, Martin and Buma, Jelle},
