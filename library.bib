@incollection{strelinNewDataRelated2006,
  title = {New {{Data Related}} to {{Holocene Landform Development}} and {{Climatic Change}} from {{James Ross Island}}, {{Antarctic Peninsula}}},
  booktitle = {Antarctica},
  author = {Strelin, Jorge A. and Sone, Toshio and Mori, Junko and Torielli, Cesar A. and Nakamura, Toshio},
