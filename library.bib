@article{bishopRemoteSensingGeomorphometry2003,
  title = {Remote Sensing and Geomorphometry for Studying Relief Production in High Mountains},
  author = {Bishop, Michael P. and Shroder, John F. and Colby, Jeffrey D.},
  date = {2003-09},
  journaltitle = {Geomorphology},
