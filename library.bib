@article{sattlerConnectionDebrisFlow2011,
  title = {On the {{Connection}} between {{Debris Flow Activity}} and {{Permafrost Degradation}}: {{A Case Study}} from the {{Schnalstal}}, {{South Tyrolean Alps}}, {{Italy}}: {{Debris Flow Activity}} and {{Permafrost Degradation}}},
