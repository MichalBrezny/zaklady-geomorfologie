@article{steadDevelopmentsCharacterizationComplex2006,
  title = {Developments in the Characterization of Complex Rock Slope Deformation and Failure Using Numerical Modelling Techniques},
  author = {Stead, D. and Eberhardt, E. and Coggan, J.S.},
