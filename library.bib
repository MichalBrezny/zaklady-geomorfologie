  title = {Slope Movements in the {{Flysch Carpathians}} of {{Eastern Czech Republic}} Triggered by Extreme Rainfalls in 1997: A Case Study},
  shorttitle = {Slope Movements in the {{Flysch Carpathians}} of {{Eastern Czech Republic}} Triggered by Extreme Rainfalls in 1997},
