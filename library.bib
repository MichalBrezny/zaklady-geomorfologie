@article{persaudActiveDeformationEastern2004,
  title = {Active Deformation in the Eastern {{Swiss Alps}}: Post-Glacial Faults, Seismicity and Surface Uplift},
  shorttitle = {Active Deformation in the Eastern {{Swiss Alps}}},
  author = {Persaud, M and Pfiffner, O.A},
  date = {2004-07},
