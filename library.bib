@article{johnsonSantiagoLandslideAssociated2005,
  title = {The {{Santiago Landslide}} and {{Associated Ridge-top Graben}} ({{Sackungen}}): {{Implications}} for {{Paleoseismic Landslide Studies}}},
  shorttitle = {The {{Santiago Landslide}} and {{Associated Ridge-top Graben}} ({{Sackungen}})},
  author = {Johnson, P. L.},
  date = {2005-02-01},
  journaltitle = {Environmental and Engineering Geoscience},
  shortjournal = {Environmental and Engineering Geoscience},
  volume = {11},
