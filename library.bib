@article{vavroPropertiesDurabilityUse2016,
  title = {Properties, Durability and Use of Glauconitic {{Godula}} Sandstones: A Relatively Less Known Building Stone of the {{Czech Republic}} and {{Poland}}},
  shorttitle = {Properties, Durability and Use of Glauconitic {{Godula}} Sandstones},
  author = {Vavro, Martin and Vavro, Leona and Martinec, Petr and Souček, Kamil},
  date = {2016-11-09},
  journaltitle = {Environmental Earth Sciences},
  shortjournal = {Environ Earth Sci},
  volume = {75},
