@article{nowosadBelgToolCalculating2020,
  title = {Belg: {{A Tool}} for {{Calculating Boltzmann Entropy}} of {{Landscape Gradients}}},
  shorttitle = {Belg},
  author = {Nowosad, Jakub and Gao, Peichao},
  date = {2020-08-26},
