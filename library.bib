  doi = {10.1016/j.tecto.2009.02.019},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0040195109001139},
  urldate = {2020-01-10},
  abstract = {Deep-seated gravitational slope deformations (DSGSDs) influence landscape development in tectonically active mountain ranges. Nevertheless, the relationships among tectonics, DSGSDs, and topography are poorly known. In this paper, the distribution of DSGSDs and their relationships with tectonic structures and active processes, surface processes, and topography were investigated at different scales. Over 100 DSGSDs were mapped in a 5000 km2 sector of the central Eastern Alps between the Valtellina, Engadine and Venosta valleys. Detailed lineament mapping was carried out by photo-interpretation in a smaller area (about 750 km2) including the upper Valtellina and Val Venosta. Fault populations were also analysed in the field and their mechanisms unravelled, allowing to identify different structural stages, the youngest being consistent with the regional pattern of the ongoing crustal deformation. Finally, four DSGSD examples have been investigated in detail by geological and 2D geomechanical modelling.},
  langid = {english},
  keywords = {\#nosource,Active tectonics,Central Eastern Alps,Deep-seated gravitational slope deformation,DSGSD,Faulting,Numerical modelling,Relief,sackung},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Tectonophysics\\2009\\Agliardi et al_2009_Tectonic vs.pdf;D\:\\Brezny\\Zotero\\storage\\WFB6QECU\\S0040195109001139.html}
}

