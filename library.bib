@article{nishiizumiAbsoluteCalibration10Be2007,
  title = {Absolute Calibration of {{10Be AMS}} Standards},
  author = {Nishiizumi, Kunihiko and Imamura, Mineo and Caffee, Marc W. and Southon, John R. and Finkel, Robert C. and McAninch, Jeffrey},
