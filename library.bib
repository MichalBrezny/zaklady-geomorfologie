@article{binnieTectonicUpliftThreshold2007,
  title = {Tectonic Uplift, Threshold Hillslopes, and Denudation Rates in a Developing Mountain Range},
  author = {Binnie, Steven A. and Phillips, William M. and Summerfield, Michael A. and Fifield, L. Keith},
  date = {2007},
  journaltitle = {Geology},
