@article{margielewskiNeolithicYewBow2010,
  title = {A {{Neolithic}} Yew Bow in the {{Polish Carpathians}}},
  author = {Margielewski, Włodzimierz and Krąpiec, Marek and Valde-Nowak, Paweł and Zernitskaya, Valentina},
  date = {2010-03},
  journaltitle = {CATENA},
  shortjournal = {CATENA},
