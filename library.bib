@article{kariyaLandslideinducedTerminalMorainelike2011,
  title = {Landslide-Induced Terminal Moraine-like Landforms on the East Side of {{Mount Shiroumadake}}, {{Northern Japanese Alps}}},
  author = {Kariya, Yoshihiko and Sato, Go and Komori, Jiro},
  date = {2011-04},
  journaltitle = {Geomorphology},
  shortjournal = {Geomorphology},
  volume = {127},
