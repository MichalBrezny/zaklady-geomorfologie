@article{picozziMomentEnergyMagnitudes2019,
  title = {Moment and Energy Magnitudes: Diversity of Views on Earthquake Shaking Potential and Earthquake Statistics},
  shorttitle = {Moment and Energy Magnitudes},
  author = {Picozzi, M and Bindi, D and Spallarossa, D and Oth, A and Di~Giacomo, D and Zollo, A},
  date = {2019-02-01},
  journaltitle = {Geophysical Journal International},
  volume = {216},
