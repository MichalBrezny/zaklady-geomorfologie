@article{simonTectonicBeheadingFluvial2013,
  title = {Tectonic Beheading of Fluvial Valleys in the {{Maestrat}} Grabens (Eastern {{Spain}}): {{Insights}} into Slip Rates of {{Pleistocene}} Extensional Faults},
  shorttitle = {Tectonic Beheading of Fluvial Valleys in the {{Maestrat}} Grabens (Eastern {{Spain}})},
  author = {Simón, José L. and Pérez-Cueva, Alejandro J. and Calvo-Cases, Adolfo},
  date = {2013-05},
  journaltitle = {Tectonophysics},
  shortjournal = {Tectonophysics},
