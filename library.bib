@article{megeGravityTectonicsTopographic2013,
  title = {Gravity Tectonics of Topographic Ridges: {{Halokinesis}} and Gravitational Spreading in the Western {{Ogaden}}, {{Ethiopia}}},
  author = {Mège, Daniel and Le Deit, Laetitia and Rango, Tewodros and Korme, Tesfaye},
  date = {2013},
  journaltitle = {Geomorphology},
  volume = {193},
