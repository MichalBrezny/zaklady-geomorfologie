  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Quaternary Research\\2013\\Pánek et al_2013_Holocene reactivations of catastrophic complex flow-like landslides in the.pdf}
}

