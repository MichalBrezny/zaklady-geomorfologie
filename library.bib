@article{bachmannAnalysisMassifFracturing2009,
  title = {Analysis of Massif Fracturing during {{Deep-Seated Gravitational Slope Deformation}} by Physical and Numerical Modeling},
  author = {Bachmann, D. and Bouissou, S. and Chemenda, A.},
  date = {2009-01},
  journaltitle = {Geomorphology},
  shortjournal = {Geomorphology},
  volume = {103},
