@article{grohmannMultiscaleAnalysisTopographic2011,
  title = {Multiscale {{Analysis}} of {{Topographic Surface Roughness}} in the {{Midland Valley}}, {{Scotland}}},
  author = {Grohmann, Carlos Henrique and Smith, Mike J. and Riccomini, Claudio},
  date = {2011-04},
  journaltitle = {IEEE Transactions on Geoscience and Remote Sensing},
