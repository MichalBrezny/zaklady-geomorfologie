@article{burbankPartitioningIntermontaneBasins1999,
  title = {Partitioning of Intermontane Basins by Thrust‐related Folding, {{Tien Shan}}, {{Kyrgyzstan}}},
  author = {{Burbank} and {McLean} and {Bullen} and {Abdrakhmatov} and {Miller}},
  date = {1999-03},
  journaltitle = {Basin Research},
