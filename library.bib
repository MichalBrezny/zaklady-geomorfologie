  keywords = {high mountain,Mont Blanc massif,permafrost,photographs,post-'Little Ice Age' period,rockfall,rockwall},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\The Holocene\\2011\\Ravanel_Deline_2011_Climate influence on rockfalls in high-Alpine steep rockwalls.pdf}
