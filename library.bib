@article{panekRainfallinducedLandslideEvent2011,
  title = {Rainfall-Induced Landslide Event of {{May}} 2010 in the Eastern Part of the {{Czech Republic}}},
  author = {Pánek, Tomáš and Brázdil, Rudolf and Klimeš, Jan and Smolková, Veronika and Hradecký, Jan and Zahradníček, Pavel},
  date = {2011-12},
  journaltitle = {Landslides},
