@article{minarNewMorphostructuralSubdivision2011,
  title = {New Morphostructural Subdivision of the {{Western Carpathians}}: {{An}} Approach Integrating Geodynamics into Targeted Morphometric Analysis},
  shorttitle = {New Morphostructural Subdivision of the {{Western Carpathians}}},
  author = {Minár, Jozef and Bielik, Miroslav and Kováč, Michal and Plašienka, Dušan and Barka, Ivan and Stankoviansky, Miloš and Zeyen, Hermann},
  date = {2011-04},
  journaltitle = {Tectonophysics},
