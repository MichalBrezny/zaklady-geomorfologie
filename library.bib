@article{woodLandslideInventoriesClimate2015,
  title = {Landslide Inventories for Climate Impacts Research in the {{European Alps}}},
  author = {Wood, J.L. and Harrison, S. and Reinhardt, L.},
  date = {2015-01},
  journaltitle = {Geomorphology},
