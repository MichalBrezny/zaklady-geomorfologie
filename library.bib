@article{suskiLocalizationCharacterizationActive2010,
  title = {Localization and Characterization of an Active Fault in an Urbanized Area in Central {{Guatemala}} by Means of Geoelectrical Imaging},
  author = {Suski, Barbara and Brocard, Gilles and Authemayou, Christine and Muralles, Beatriz Consenza and Teyssier, Christian and Holliger, Klaus},
