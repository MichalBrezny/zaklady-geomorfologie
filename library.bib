@article{fentonRegional10BeProduction2011,
  title = {Regional {{10Be}} Production Rate Calibration for the Past 12ka Deduced from the Radiocarbon-Dated {{Grøtlandsura}} and {{Russenes}} Rock Avalanches at 69° {{N}}, {{Norway}}},
  author = {Fenton, Cassandra R. and Hermanns, Reginald L. and Blikra, Lars H. and Kubik, Peter W. and Bryant, Charlotte and Niedermann, Samuel and Meixner, Anette and Goethals, Mirjam M.},
  date = {2011},
