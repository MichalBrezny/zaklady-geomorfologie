@article{beladjineCollisionProcessIncident2007,
  title = {Collision Process between an Incident Bead and a Three-Dimensional Granular Packing},
  author = {Beladjine, Djaoued and Ammi, Madani and Oger, Luc and Valance, Alexandre},
  date = {2007-06-14},
  journaltitle = {Physical Review E},
