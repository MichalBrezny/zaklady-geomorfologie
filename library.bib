@article{hackerCatastrophicEmplacementGigantic2014,
  title = {Catastrophic Emplacement of the Gigantic {{Markagunt}} Gravity Slide, Southwest {{Utah}} ({{USA}}): {{Implications}} for Hazards Associated with Sector Collapse of Volcanic Fields},
  shorttitle = {Catastrophic Emplacement of the Gigantic {{Markagunt}} Gravity Slide, Southwest {{Utah}} ({{USA}})},
