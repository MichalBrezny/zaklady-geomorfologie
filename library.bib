  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Natural Hazards and Earth System Sciences\\2012\\Fischer et al_2012_On the influence of topographic, geological and cryospheric factors on rock.pdf}
