@article{matsuokaFrostWeatheringRockwall2008,
  title = {Frost Weathering and Rockwall Erosion in the Southeastern {{Swiss Alps}}: {{Long-term}} (1994–2006) Observations},
  shorttitle = {Frost Weathering and Rockwall Erosion in the Southeastern {{Swiss Alps}}},
  author = {Matsuoka, Norikazu},
  date = {2008-07},
  journaltitle = {Geomorphology},
  shortjournal = {Geomorphology},
  volume = {99},
  number = {1-4},
  pages = {353--368},
  issn = {0169555X},
  doi = {10.1016/j.geomorph.2007.11.013},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0169555X07005351},
  urldate = {2020-01-10},
  abstract = {Rates and processes of frost weathering in the Alps were investigated by visual observations of intensively shattered rocks, continuous monitoring of frost wedging and rock temperatures in bedrock and measurements of rockfall activity. Rapid frost weathering of hard-intact rocks occurs along lakes and streams where seasonal freezing promotes ice segregation in the rock. Otherwise, rocks require pre-existing weakness or a long exposure period for intensively shattered. Automated monitoring shows that crack opening occurs at three scales, including small opening accompanying short-term frost cycles, slightly larger movements during seasonal freezing and occasional large opening originating from refreezing of snow-melt water during seasonal thawing. The opening events require at least partial water saturation in the crack. The repetition of crack opening (frost wedging) results in permanent opening and finally debris dislocation. Debris collections below fractured rockwalls show that pebble falls occur at an average rate of about 0.1 mm a−1 with significant spatial and inter-annual variations. Occasional large boulder falls significantly raise the rockwall erosion rates, controlled by such factors as the joint distribution in the bedrock, repetition of annual freeze–thaw cycles and extraordinary summer thaw.},
  langid = {english},
  keywords = {field monitoring,frost weathering,periglacial processes,rock slope instability,rockfall,swiss alps},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Geomorphology\\2008\\Matsuoka_2008_Frost weathering and rockwall erosion in the southeastern Swiss Alps.pdf}
}

@article{matsuokaRockfallActivityAlpine1999,
  title = {Rockfall Activity from an Alpine Cliff during Thawing Periods},
  author = {Matsuoka, Norikazu and Sakai, Hiroaki},
  date = {1999-07},
  journaltitle = {Geomorphology},
  shortjournal = {Geomorphology},
  volume = {28},
  number = {3-4},
  pages = {309--328},
  issn = {0169555X},
  doi = {10/bmx4v5},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0169555X98001160},
  urldate = {2020-01-10},
  abstract = {Rockfall activity during thawing periods was investigated by collecting rockfall debris fallen on the snow-covered talus slope in a cirque in the Japanese Alps. Near-surface rock temperature was also monitored on the cirque wall. Maximum rockfall activity occurs on average about 10 days after the meltout of the cirque wall. The intensive activity is rarely associated with precipitation events or diurnal frost cycles on the rockface. A thermal conduction model suggests that rockfalls at this site occur most frequently when the seasonal thawing front penetrates to a depth of about 1 m in the bedrock. While the freeze–thaw penetration controls the maximum dimensions of detachable rock mass, joint spacing on the rockface affects the size distribution of rockfall debris. Boulder falls resulting from seasonal frost weathering are considered to be the most important process responsible for the contemporary modification of the cirque wall. The rockfall volume during a thawing period is usually 1–3 m3, which is equivalent to the rate of cirque wall retreat of the order of 0.01 mm yeary1. However, big boulder falls that occur once per decade can increase the retreat rate significantly. q 1999 Elsevier Science B.V. All rights reserved.},
