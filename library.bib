@article{lendaUseSurfaceInterpolation2016,
  title = {The Use of Surface Interpolation Methods for Landslides Monitoring},
  author = {Lenda, Grzegorz and Ligas, Marcin and Lewińska, Paulina and Szafarczyk, Anna},
