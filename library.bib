@article{donatiPreliminaryInvestigationRole2019,
  title = {A {{Preliminary Investigation}} on the {{Role}} of {{Brittle Fracture}} in the {{Kinematics}} of the 2014 {{San Leo Landslide}}},
  author = {Donati, Davide and Stead, Doug and Elmo, Davide and Borgatti, Lisa},
  date = {2019-06-07},
