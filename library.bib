@article{elmoApplicationsFiniteDiscrete2013,
  title = {Applications of {{Finite}}/{{Discrete Element Modeling}} to {{Rock Engineering Problems}}},
  author = {Elmo, Davide and Stead, Doug and Eberhardt, Erik and Vyazmensky, Alex},
  date = {2013-10},
  journaltitle = {International Journal of Geomechanics},
