@article{froudeGlobalFatalLandslide2018,
  title = {Global Fatal Landslide Occurrence from 2004 to 2016},
  author = {Froude, Melanie J. and Petley, David N.},
  date = {2018-08-23},
  journaltitle = {Natural Hazards and Earth System Sciences},
  shortjournal = {Nat. Hazards Earth Syst. Sci.},
  volume = {18},
  number = {8},
  pages = {2161--2181},
  issn = {1684-9981},
