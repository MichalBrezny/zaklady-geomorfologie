@article{klemanPatternsQuaternaryIce2008,
  title = {Patterns of {{Quaternary}} Ice Sheet Erosion and Deposition in {{Fennoscandia}} and a Theoretical Framework for Explanation},
  author = {Kleman, Johan and Stroeven, Arjen P. and Lundqvist, Jan},
