@article{lenartCommentsStructuralTectonic2014,
  title = {Comments on “Structural–Tectonic Controls and Geomorphology of the Karst Corridors in Alpine Limestone Ridges: {{Southern Carpathians}}, {{Romania}}” by {{L}}. {{Tîrlă}} and {{I}}. {{Vijulie}}, {{Geomorphology}} 197 (2013), 123–136},
  shorttitle = {Comments on “structural–Tectonic Controls and Geomorphology of the Karst Corridors in Alpine Limestone Ridges},
  author = {Lenart, Jan and Pánek, Tomáš},
  date = {2014-10},
  journaltitle = {Geomorphology},
  shortjournal = {Geomorphology},
  volume = {223},
  pages = {90--95},
  issn = {0169555X},
  doi = {10/f6gw8g},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0169555X14003547},
  urldate = {2020-01-10},
  abstract = {In the recently published paper by Tîrlă and Vijulie (2013), the authors interpret the abundant trenches affecting the limestone ridge of the Vânturariţa-Buila Massif (Southern Carpathians, Romania) as bogaz-like landforms (syn. kluftkarren, karst corridors, karst streets, zanjones etc.). These karst features originated, according to the authors, through the corrosion of structurally anisotropic limestone bedrock. This proposed origin is supported by a comparison of the rose diagrams of the directions of both bogaz and tectonic discontinuities, by a morphometric analysis and by observations of the speleothem remnants covering the exposed fault surfaces. Furthermore, the authors described the typically karstic settings of the area, such as extremely poor surface drainage, subterranean connections between the karst corridors and karst springs in pocket valleys. We disagree with their main interpretations and argue that the presumed bogaz are gravitationally-induced grabens and trenches created by mass movement activity, namely by deep-seated slope failures. The speleothem remnants are interpreted as flowstones deposited during the subsurface widening of the joints by gravitational activity. These speleothem remnants became fossil (and even rotated from the original position) after sufficient widening of the joints and subsequent exposure of the crevices to the surface due to slope failure progression. The morphology of the slope failure along the Vânturariţa-Buila Massif is similar to numerous other elevated ridges within the Carpathian Mountains.},
  langid = {english},
  keywords = {Bogaz,Crevice-type caves,Karst corridor,Mass movements,Slope failures,Southern Carpathians},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Geomorphology\\2014\\Lenart_Pánek_2014_Comments on “structural–tectonic controls and geomorphology of the karst.pdf}
}

@article{lenartComplexLandslidesDiscrepancy2021,
  title = {Complex Landslides: Discrepancy between Varied Partial Movement Mechanisms Detected in Crevice-Type Caves and by Formal Investigation},
  shorttitle = {Complex Landslides},
  author = {Lenart, Jan and Horáček, Matěj},
  date = {2021-02-01},
  journaltitle = {Bulletin of Engineering Geology and the Environment},
  shortjournal = {Bull Eng Geol Environ},
  volume = {80},
  number = {2},
  pages = {979--992},
  issn = {1435-9537},
  doi = {10/gngnxx},
  url = {https://doi.org/10.1007/s10064-020-02033-0},
  urldate = {2021-02-09},
  abstract = {Mass movements can be investigated usually only from the surface or, more in-depth, indirectly by geophysical methods and by spot drilling. Crevice-type caves allow us to visit and observe the inner environment of the slope deformations. Such caves are typically composed of systems comprising long and high joint-predisposed corridors, enabling us to investigate geological outcrops hundreds of meters long and tens of meters deep within the landslide mass. The present paper shows the diversity of particular gravitational movements influencing the vast slid rock blocks, which seem to remain intact and consolidated within the landslide body. By use of geological compass measurement and statistical evaluation, we detect complex movement mechanisms in most investigated cases, often without any impact on the topography above. The results prove the existence of a great difference between the overall behavior of the deformation and its individual rock blocks, as well as discrepancy between movement mechanisms detected in crevice-type caves and by formal investigation. Furthermore, this paper discusses the related horizontal rotation and subsidence movements documented by previous studies. We can conclude that most deformations are complex.},
  langid = {english},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Bulletin of Engineering Geology and the Environment\\2021\\Lenart_Horáček_2021_Complex landslides.pdf}
}

