@article{zangerlKinematicBehaviourVelocity2010,
  title = {Kinematic Behaviour and Velocity Characteristics of a Complex Deep-Seated Crystalline Rockslide System in Relation to Its Interaction with a Dam Reservoir},
  author = {Zangerl, C. and Eberhardt, E. and Perzlmaier, S.},
