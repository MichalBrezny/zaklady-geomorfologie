@article{raskaUsingLocalArchive2015,
  title = {Using {{Local Archive Sources}} to {{Reconstruct Historical Landslide Occurrence}} in {{Selected Urban REgions}} of the {{Czech Republic}}: {{Examples}} from {{Regions}} with {{Different Historical Development}}},
  shorttitle = {Using {{Local Archive Sources}} to {{Reconstruct Historical Landslide Occurrence}} in {{Selected Urban REgions}} of the {{Czech Republic}}},
  author = {Raska, P. and Klimes, J. and Dubisar, J.},
  date = {2015-02},
  journaltitle = {Land Degradation \& Development},
  shortjournal = {Land Degrad. Develop.},
