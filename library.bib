  keywords = {cal bp,climate-change,coprophilous fungi,events,history,holocene,human impact,landslide peat bogs,middle and late holocene,netherlands,palaeoenvironmental changes,polish flysch carpathians,radiocarbon-dates,sediments,swiss alps},
