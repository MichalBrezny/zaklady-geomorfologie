@article{schuurVulnerabilityPermafrostCarbon2008,
  title = {Vulnerability of {{Permafrost Carbon}} to {{Climate Change}}: {{Implications}} for the {{Global Carbon Cycle}}},
  shorttitle = {Vulnerability of {{Permafrost Carbon}} to {{Climate Change}}},
  author = {Schuur, Edward A. G. and Bockheim, James and Canadell, Josep G. and Euskirchen, Eugenie and Field, Christopher B. and Goryachkin, Sergey V. and Hagemann, Stefan and Kuhry, Peter and Lafleur, Peter M. and Lee, Hanna and Mazhitova, Galina and Nelson, Frederick E. and Rinke, Annette and Romanovsky, Vladimir E. and Shiklomanov, Nikolay and Tarnocai, Charles and Venevsky, Sergey and Vogel, Jason G. and Zimov, Sergei A.},
