  keywords = {DIPANALYST,Kinematic analysis,Rock slopes,Slope stability,Stereonets},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Computers & Geosciences\\2013\\Admassu_Shakoor_2013_DIPANALYST.pdf}
