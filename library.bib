@article{bertiBackAnalysisLarge2017a,
  title = {Back Analysis of a Large Landslide in a Flysch Rock Mass},
  author = {Berti, Matteo and Bertello, Lara and Bernardi, Anna Rita and Caputo, Giuseppe},
  date = {2017-12-01},
  journaltitle = {Landslides},
