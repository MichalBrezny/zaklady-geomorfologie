@article{argyriouGISMulticriteriaDecision2016,
  title = {{{GIS}} Multi-Criteria Decision Analysis for Assessment and Mapping of Neotectonic Landscape Deformation: {{A}} Case Study from {{Crete}}},
  shorttitle = {{{GIS}} Multi-Criteria Decision Analysis for Assessment and Mapping of Neotectonic Landscape Deformation},
  author = {Argyriou, Athanasios V. and Teeuw, Richard M. and Rust, Derek and Sarris, Apostolos},
