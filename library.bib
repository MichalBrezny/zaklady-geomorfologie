@article{tichavskyIncreasedGullyActivity2018,
  title = {Increased Gully Activity Induced by Short-Term Human Interventions – {{Dendrogeomorphic}} Research Based on Exposed Tree Roots},
  author = {Tichavský, Radek and Kluzová, Olga and Břežný, Michal and Ondráčková, Lenka and Krpec, Petr and Tolasz, Radim and Šilhán, Karel},
