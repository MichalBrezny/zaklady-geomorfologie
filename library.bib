@article{yinLandslideHazardsTriggered2009,
  title = {Landslide Hazards Triggered by the 2008 {{Wenchuan}} Earthquake, {{Sichuan}}, {{China}}},
  author = {Yin, Yueping and Wang, Fawu and Sun, Ping},
