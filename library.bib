@article{jordanMorphometricAnalysisTectonic2003,
  title = {Morphometric Analysis and Tectonic Interpretation of Digital Terrain Data: A Case Study},
  shorttitle = {Morphometric Analysis and Tectonic Interpretation of Digital Terrain Data},
  author = {Jordan, Gyozo},
  date = {2003-08},
  journaltitle = {Earth Surface Processes and Landforms},
