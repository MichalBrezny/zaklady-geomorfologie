@article{cernaSurfaceSubsurfaceSchmidt2011,
  title = {Surface and Sub-Surface {{Schmidt}} Hammer Rebound Value Variation for a Granite Outcrop},
  author = {Černá, Barbora and Engel, Zbyněk},
  date = {2011-02},
  journaltitle = {Earth Surface Processes and Landforms},
