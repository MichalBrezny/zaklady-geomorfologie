@article{pedrazziniStructuralAnalysisTurtle2011,
  title = {Structural Analysis of {{Turtle Mountain}}: Origin and Influence of Fractures in the Development of Rock Slope Failures},
  shorttitle = {Structural Analysis of {{Turtle Mountain}}},
  author = {Pedrazzini, Andrea and Jaboyedoff, Michel and Froese, Corey R. and Langenberg, C. Willem and Moreno, Francisco},
  date = {2011-01-01},
  journaltitle = {Geological Society, London, Special Publications},
