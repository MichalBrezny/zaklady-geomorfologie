@article{mondiniCombiningMultipleChange2011,
  title = {Combining Multiple Change Detection Indices for Mapping Landslides Triggered by Typhoons},
  author = {Mondini, Alessandro C. and Chang, Kang-Tsung and Yin, Hsiao-Yuan},
  date = {2011-11},
