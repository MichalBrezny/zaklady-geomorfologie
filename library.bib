  title = {Mapping Landslide Susceptibility from Small Datasets: {{A}} Case Study in the {{Pays}} de {{Herve}} ({{E Belgium}})},
  shorttitle = {Mapping Landslide Susceptibility from Small Datasets},
  author = {Demoulin, Alain and Chung, Chang-Jo F.},
