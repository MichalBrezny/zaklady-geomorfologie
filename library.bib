  title = {System for {{Automated Geoscientific Analyses}} ({{SAGA}}) v. 2.1.4},
  author = {Conrad, O. and Bechtel, B. and Bock, M. and Dietrich, H. and Fischer, E. and Gerlitz, L. and Wehberg, J. and Wichmann, V. and Böhner, J.},
