@article{dustConceptualModelComplex2012,
  title = {Conceptual Model for Complex River Responses Using an Expanded {{Lane}}'s Relation},
  author = {Dust, David and Wohl, Ellen},
  year = {2012},
  month = feb,
  journal = {Geomorphology},
  volume = {139--140},
  pages = {109--121},
  issn = {0169-555X},
  doi = {10/c739dh},
  urldate = {2021-12-23},
  abstract = {In 1955, Lane introduced the qualitative expression QwS {$\alpha$} QsDs to relate water discharge (Qw), channel slope (S), sediment discharge or load (Qs), and a representative bed sediment size (Ds) for a river reach under dynamic equilibrium conditions. For over 50years, Lane's relation has provided a unique conceptual model useful to engineers, geomorphologists, and educators for visualizing and describing the concept of dynamic equilibrium in rivers. However, the ability of Lane's relation to describe river adjustments is inherently limited because the expression cannot account for the changes in cross-sectional, planform and bedform geometry that are typically associated with complex channel adjustments. By demonstrating that Qs is inversely proportional to the width-to-depth ratio (W/d), we expand Lane's relation to include the width-to-depth ratio as a cross-sectional geometry term: QwS {$\alpha$} QsDs (W/d). We further demonstrate that the slope term (S) is proportional to the total change in elevation along a channel ({$\increment$}z) and inversely proportional to sinuosity (P) and bedform amplitude (H\textasciimacron a): S {$\alpha$} {$\increment$}z/(PH\textasciimacron a). The introduction of the width-to-depth ratio and expressing channel slope in terms of channel geometry expands the original Lane's relation to explicitly include measures of cross-sectional, planform and bedform geometry as additional degrees of freedom. This significantly broadens the usefulness of Lane's original relation as a conceptual model and allows the visualization of complex river responses that commonly involve adjustments to cross-sectional, planform, and/or bedform geometry.},
  langid = {english},
  file = {C\:\\Users\\Brezny\\OneDrive - Ostravska univerzita\\Zotero_attachments\\Journal Article\\Geomorphology\\2012\\Dust_Wohl_2012_Conceptual model for complex river responses using an expanded Lane's relation.pdf}
}

